<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nolla.dseknolla"
    android:versionCode="4"
    android:versionName="1.2" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.nolla.dseknolla.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nolla.dseknolla.CalendarActivity"
            android:label="@string/title_activity_calendar"
            android:parentActivityName="com.nolla.dseknolla.MainActivity" >
        </activity>
        <activity
            android:name="com.nolla.dseknolla.NewsActivity"
            android:label="@string/title_activity_news"
            android:parentActivityName="com.nolla.dseknolla.MainActivity" >
        </activity>
        <activity
            android:name="com.nolla.dseknolla.Ordlista"
            android:label="@string/title_activity_ordlista"
            android:parentActivityName="com.nolla.dseknolla.MainActivity" >
        </activity>
        <activity
            android:name="com.nolla.dseknolla.LoadingActivity"
            android:label="@string/title_activity_loading" >
        </activity>
        <activity
            android:name="com.nolla.dseknolla.CampusMap"
            android:label="@string/title_activity_map" >
        </activity>
        <activity
            android:name="com.nolla.dseknolla.MapChooser"
            android:label="@string/title_activity_map_chooser"
            android:parentActivityName="com.nolla.dseknolla.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.nolla.dseknolla.MainActivity" />
        </activity>
        <activity
            android:name="com.nolla.dseknolla.InfoAndLinks"
            android:label="@string/title_activity_info_and_links"
            android:parentActivityName="com.nolla.dseknolla.MainActivity" >
 >
        </activity>
        <activity
            android:name="com.nolla.dseknolla.InfoPane"
            android:label="@string/title_activity_info_pane"
            android:parentActivityName="com.nolla.dseknolla.InfoAndLinks" >
>
        </activity>
        <activity
            android:name="com.nolla.dseknolla.Hugo"
            android:label="@string/title_activity_hugo"
            android:parentActivityName="com.nolla.dseknolla.MainActivity" >
        </activity>
        <activity
            android:name="com.nolla.dseknolla.MapChooser2"
            android:label="@string/title_activity_map_chooser2" >
        </activity>
    </application>

